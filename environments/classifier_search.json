{ 
    "LAZY_DATASET_READER": 0,
    "CUDA_DEVICE": 0,
    "EVALUATE_ON_TEST": 0,
    "NUM_EPOCHS": 50,
    "SEED": {
        "sampling strategy": "integer",
        "bounds": [0, 100000]
    },
    "DATA_DIR": "s3://suching-dev/final-datasets/imdb/",
    "THROTTLE": 200,
    "USE_SPACY_TOKENIZER": 0,
    "FREEZE_EMBEDDINGS": ["VAMPIRE"],
    "EMBEDDINGS": ["VAMPIRE", "GLOVE"],
    "ENCODER": {
        "sampling strategy": "choice",
        "choices": ["LSTM"]
    },
    "LEARNING_RATE": {
        "sampling strategy": "loguniform",
        "bounds": [1e-6, 1e-1]
    },
    "DROPOUT": {
        "sampling strategy": "uniform",
        "bounds": [0, 0.5]
    },
    "EMBEDDING_DROPOUT": {
        "sampling strategy": "uniform",
        "bounds": [0, 0.5]
    },
    "BATCH_SIZE": 32,
    "NUM_ENCODER_LAYERS": {
        "sampling strategy": "choice",
        "choices": [1, 2, 3]
    },
    "NUM_OUTPUT_LAYERS": {
        "sampling strategy": "choice",
        "choices": [1, 2, 3]
    },
    "MAX_FILTER_SIZE": {
        "sampling strategy": "integer",
        "bounds": [3, 6]
    },
    "NUM_FILTERS": {
        "sampling strategy": "integer",
        "bounds": [64, 512]
    },
    "HIDDEN_SIZE": {
        "sampling strategy": "integer",
        "bounds": [64, 512]
    },
    "AGGREGATIONS": {
        "sampling strategy": "subset",
        "choices": ["maxpool", "meanpool", "attention", "final_state"]
    },
    "MAX_CHARACTER_FILTER_SIZE": {
        "sampling strategy": "integer",
        "bounds": [3, 6]
    },
    "NUM_CHARACTER_FILTERS": {
        "sampling strategy": "integer",
        "bounds": [16, 64]
    },
    "CHARACTER_HIDDEN_SIZE": {
        "sampling strategy": "integer",
        "bounds": [16, 128]
    },
    "CHARACTER_EMBEDDING_DIM": {
        "sampling strategy": "integer",
        "bounds": [16, 128]
    },
    "CHARACTER_ENCODER": {
        "sampling strategy": "choice",
        "choices": ["LSTM", "CNN", "AVERAGE"]
    },
    "NUM_CHARACTER_ENCODER_LAYERS": {
        "sampling strategy": "choice",
        "choices": [1, 2]
    }
}